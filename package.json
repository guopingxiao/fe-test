{
  "name": "koa2-startkit",
  "version": "0.2.0",
  "author": "wssgcg1213",
  "scripts": {
    "start": "node bin/development.js",
    "production": "node bin/production.js",
    "debug": "npm run clean && node bin/debug",
    "build": "npm test && npm run clean && npm run compile",
    "compile": "babel src/ --out-dir app/ --retain-lines --source-maps",
    "clean": "rm -rf app/",
    "test": "mocha -u bdd --compilers js:babel-core/register",
    "dev": "npm start",
    "jest-assert":"jest __tests__/assert.test.js",
    "jest-mock":"jest __tests__/mock.test.js",
    "jest-mockFn":"jest __tests__/mockFn.test.js"
  },
  "engines": {
    "node": ">= 4"
  },
  "dependencies": {
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.7",
    "babel-plugin-add-module-exports": "^0.1.4",
    "babel-plugin-transform-runtime": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-preset-stage-3": "^6.5.0",
    "babel-runtime": "^6.6.1",
    "co": "^4.6.0",
    "debug": "^2.2.0",
    "ejs": "^2.4.1",
    "koa": "^2.0.0",
    "koa-bodyparser": "^2.0.1",
    "koa-convert": "^1.2.0",
    "koa-json": "^1.1.1",
    "koa-logger": "^1.3.0",
    "koa-onerror": "^3.0.1",
    "koa-router": "^7.0.0",
    "koa-static-plus": "^0.1.1",
    "koa-views": "^5.0.1",
    "lodash": "^4.11.1"
  },
  "devDependencies": {
    "chokidar": "^1.5.1",
    "colors": "^1.1.2",
    "mocha": "^2.4.5",
    "should": "^8.3.0",
    "supertest": "^1.2.0"
  },
